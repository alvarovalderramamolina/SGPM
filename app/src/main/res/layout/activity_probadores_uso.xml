<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="50dp"
    tools:context=".ProbadoresUso"
    tools:visibility="visible">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="1300dp"
        android:layout_height="127dp"
        android:layout_marginBottom="673dp"
        android:background="#202F3F "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="235dp"
            android:layout_height="54dp"
            android:layout_marginEnd="518dp"
            android:layout_marginBottom="37dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/logo_blanco" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="255dp"
            android:layout_height="87dp"
            android:layout_marginEnd="246dp"
            android:layout_marginBottom="20dp"
            android:background="#202F3F "
            android:tint="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView8"
            app:srcCompat="@drawable/boton_salir" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="323dp"
        android:layout_height="62dp"
        android:layout_marginEnd="916dp"
        android:layout_marginBottom="588dp"
        android:text="Probadores en uso"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="254dp"
        android:layout_height="35dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="243dp"
        android:layout_marginEnd="137dp"
        android:text="Probador 1"
        android:textColor="#070707"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="254dp"
        android:layout_height="35dp"
        android:layout_marginEnd="141dp"
        android:layout_marginBottom="284dp"
        android:text="Probador 2"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="254dp"
        android:layout_height="35dp"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="283dp"
        android:text="Probador 3"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="254dp"
        android:layout_height="35dp"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="16dp"
        android:text="Probador 4"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="331dp"
        android:layout_height="237dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="64dp"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:isScrollContainer="false"
        android:scaleType="fitXY"
        android:scaleX="1"
        android:scaleY="1"
        app:layout_constraintEnd_toStartOf="@+id/imageButton8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/probador5" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="337dp"
        android:layout_height="237dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="89dp"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/probador9" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="331dp"
        android:layout_height="237dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="64dp"
        android:cropToPadding="false"
        android:saveEnabled="false"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/imageButton9"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:srcCompat="@drawable/probador10" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="331dp"
        android:layout_height="237dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="135dp"
        android:cropToPadding="false"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:srcCompat="@drawable/probadores11" />

</android.support.constraint.ConstraintLayout>